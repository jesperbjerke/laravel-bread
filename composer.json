{
    "name": "bjerke/laravel-bread",
    "description": "A boilerplate package for BREAD operations through REST API in Laravel",
    "keywords": [
        "jesperbjerke",
        "bread",
        "crud",
        "api",
        "eloquent",
        "query",
        "orm",
        "rest",
        "laravel"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jesper Bjerke",
            "email": "jesper.bjerke@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json" : "*",
        "bjerke/api-query-builder": "^1.0",
        "illuminate/support": "^7.0 || ^8.0",
        "illuminate/http": "^7.0 || ^8.0",
        "illuminate/database": "^7.0 || ^8.0",
        "illuminate/routing": "^7.0 || ^8.0",
        "illuminate/validation": "^7.0 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "Bjerke\\Bread\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bjerke\\Bread\\BreadServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
